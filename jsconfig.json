{
    "compilerOptions": {
        "lib": ["dom", "es2015", "es2016", "es6", "es2017"],
        "target": "es2017",
        "module": "es6",
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "@root/*": ["*"],
            "@core/*": ["src/*", "core/*"],
            "@styles/*": ["src/styles/*", "core/styles/*"],
            "@modules/*": ["src/modules/*", "core/modules/*"],
            "@sellermodules/*": ["src/modules/seller/*", "core/modules/seller/*"],
            "@helpers/*": ["src/helpers/*", "core/helpers/*"],
            "@services/*": ["src/services/*", "core/services/*"],
            "@config": ["swift.config.js"],
            "@pjson": ["package.json"],

            "@next_headcustom": ["src/nextjs_custom/HeadCustom", "core/nextjs_custom/HeadCustom"],
            "@next_nextscriptcustom": ["src/nextjs_custom/NextScriptCustom", "core/nextjs_custom/NextScriptCustom"],

            "@middlewares/*": ["src/middlewares/*", "core/middlewares/*"],
